{
  "name": "nest-typeorm-crud-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "STAGE=local nest start --watch",
    "start:debug": "STAGE=local nest start --debug --watch",
    "start:prod": "node dist/main",
    "run:db:create": "docker-compose exec db psql -U postgres -c 'CREATE DATABASE nesttypeormcrudtemplate;'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "run:db": "docker-compose up --build -d",
    "wait-for-db": "sh -c 'echo \"Waiting for PostgreSQL to be ready...\" && sleep 5'",
    "local": "npm run run:db && npm run wait-for-db && npm run start:local",
    "typeorm": "npm run build && npx typeorm -d dist/db/data-source.js",
    "migration:generate:local": "STAGE=local npm run typeorm migration:generate",
    "migration:run:local": "npm run build && STAGE=local npx typeorm-ts-node-esm migration:run -d dist/db/data-source.js",
    "migration:revert:local": "STAGE=local npm run typeorm -- migration:revert",
    "migration:create:local": "STAGE=local npx typeorm migration:create",
    "db:sync:local": "STAGE=local npx typeorm-ts-node-esm schema:sync -d dist/db/data-source.js"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "nodemailer": "^6.9.4",
    "otp-generator": "^4.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "puppeteer": "^21.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "shelljs": "^0.8.5",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/hapi": "^18.0.8",
    "@types/hapi__joi": "^17.1.9",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.9",
    "@types/otp-generator": "^4.0.0",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}